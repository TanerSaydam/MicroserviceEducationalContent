{
  "Routes": [
    {
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "UpstreamPathTemplate": "/api/products/{catchAll}",
      "DownstreamPathTemplate": "/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "product1",
          "Port": 8080
        },
        {
          "Host": "product2",
          "Port": 8080
        }
      ],
      "RateLimitOptions": {
        "ClientIdHeader": "ClientId",
        "Limit": 100,
        "Period": "1s"
      },
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5001",
    "RateLimitOptions": {
      "HttpStatusCode": 429
    }
  }
}