{
  "Routes": [
    {
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "UpstreamPathTemplate": "/api/categories/{catchAll}",
      "DownstreamPathTemplate": "/categories/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [],
      "RateLimitOptions": {
        "ClientIdHeader": "ClientId",
        "Limit": 100,
        "Period": "1s"
      },
      "ServiceName": "CategoryWebAPI"
    },
    {
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "UpstreamPathTemplate": "/api/products/{catchAll}",
      "DownstreamPathTemplate": "/products/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        //{
        //  "Host": "localhost",
        //  "Port": 5004
        //}
      ],
      "RateLimitOptions": {
        "ClientIdHeader": "ClientId",
        "Limit": 100,
        "Period": "1s"
      },
      "ServiceName": "ProductWebAPI",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5001",
    "RateLimitOptions": {
      "HttpStatusCode": 429
    },
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "consul",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}